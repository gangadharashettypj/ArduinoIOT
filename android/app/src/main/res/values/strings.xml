    <string name="accelerometer_dialog_message">Rotate your device to control the car.\n\nThe \"stop\" command is sent when your device is on a flat position.\n\nThe \"action\" command is sent when you click on the action button.</string>

    <string name="voice_dialog_title">Information</string>
